      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bkevin1999.newui"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "output": "single",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "1ef1cc8c-076b-400a-a3ff-d861ac651902"
      }
    }
  }
}
